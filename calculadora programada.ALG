 algoritmo "semnome"
 var
 N1, N2, S: Inteiro
  inicio
      Escreval ("Informe dois numeros para a soma")
     Escreva ("informe um numero: ")
     Leia (N1)
     Escreva ("Informe outro numero: ")
     Leia (N2)
      S <- N1 + N2
      Escreva ("A soma entre ", N1, " + ", N2, " e igual a ", S)

fimalgoritmo